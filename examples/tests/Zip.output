== Functions ==
 (++) :: [Int] -> [Int] -> [Int]
  zip :: [Int] -> [Int] -> [(Int, Int)]
eqLen :: [Int] -> [Int] -> Bool

== Laws ==
  1. x = y :: ()
  2. eqLen xs ys = eqLen ys xs
  3. eqLen xs xs
  4. eqLen zs ys => eqLen xs ys = eqLen xs zs
  5. (xs ++ ys) ++ zs = xs ++ (ys ++ zs)
  6. eqLen xs (ys ++ zs) = eqLen xs (zs ++ ys)
  7. eqLen xs (xs ++ ys) = eqLen zs (zs ++ ys)
  8. eqLen ws zs => eqLen xs (ys ++ zs) = eqLen xs (ys ++ ws)
  9. zip xs (xs ++ ys) = zip xs xs
 10. zip (xs ++ ys) xs = zip xs xs
 11. eqLen xs zs => zip (xs ++ ys) zs = zip xs zs
 12. eqLen xs ys => zip xs (ys ++ zs) = zip xs ys
 13. eqLen xs (ys ++ (zs ++ ws)) = eqLen xs (ys ++ (ws ++ zs))
 14. eqLen xs (xs ++ (ys ++ ys)) = eqLen xs (xs ++ ys)
 15. eqLen (xs ++ ys) (zs ++ ys) = eqLen xs zs
 16. eqLen (xs ++ xs) (ys ++ ys) = eqLen xs ys
 17. zip xs (ys ++ (xs ++ zs)) = zip xs (ys ++ xs)
 18. zip (xs ++ (ys ++ zs)) ys = zip (xs ++ ys) ys
 19. eqLen ys ws => zip (xs ++ (ys ++ zs)) ws = zip (xs ++ ys) ws
 20. eqLen xs zs => zip xs (ys ++ (zs ++ ws)) = zip xs (ys ++ zs)

