== Functions ==
return :: a -> [a]
 (>>=) :: [a] -> (a -> [b]) -> [b]
  (++) :: [a] -> [a] -> [a]
 (>=>) :: (a -> [b]) -> (b -> [c]) -> a -> [c]

== Laws ==
  1. xs >>= return = xs
  2. return >=> f = f
  3. f >=> return = f
  4. return x >>= f = f x
  5. f x >>= g = (f >=> g) x
  6. (xs ++ ys) ++ zs = xs ++ (ys ++ zs)
  7. xs >>= (f >=> g) = (xs >>= f) >>= g
  8. (f >=> g) >=> h = f >=> (g >=> h)
  9. f x ++ (xs >>= f) = (return x ++ xs) >>= f
 10. (xs >>= f) ++ f x = (xs ++ return x) >>= f
 11. (xs >>= f) ++ (ys >>= f) = (xs ++ ys) >>= f

