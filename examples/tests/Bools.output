== Functions ==
  not :: Bool -> Bool
 True :: Bool
False :: Bool
 (||) :: Bool -> Bool -> Bool
 (&&) :: Bool -> Bool -> Bool

== Laws ==
  1. not x & not y => x = y
  2. not x => False = x
  3. not x => not True = x
  4. x && y = y && x
  5. x && x = x
  6. not y => x && y = y
  7. x || y = y || x
  8. x || x = x
  9. not y => x || y = x
 10. x && True = x
 11. not (not x) = x
 12. x || True
 13. not y => x && not x = y
 14. x || not x
 15. (x && y) && z = x && (y && z)
 16. x && (x || y) = x
 17. x || (x && y) = x
 18. (x || y) || z = x || (y || z)
 19. not x && not y = not (x || y)
 20. x && not (x && y) = x && not y
 21. x && (y || (x && z)) = x && (y || z)
 22. (x || y) && (x || z) = x || (y && z)

