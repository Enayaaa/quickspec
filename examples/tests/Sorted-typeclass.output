== Functions ==
    [] :: [a]
   (:) :: a -> [a] -> [a]
  sort :: Ord a => [a] -> [a]
insert :: Ord a => a -> [a] -> [a]
sorted :: Ord a => [a] -> Bool

== Laws ==
  1. sort [] = []
  2. sorted []
  3. insert x [] = x : []
  4. sort (sort xs) = sort xs
  5. sorted (sort xs)
  6. sort (x : xs) = insert x (sort xs)
  7. sort (insert x xs) = insert x (sort xs)
  8. sorted (insert x xs) = sorted xs
  9. insert x (x : xs) = x : (x : xs)
 10. insert x (insert y xs) = insert y (insert x xs)
 11. sorted (x : insert x xs) = sorted (x : xs)

