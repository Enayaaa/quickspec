== Functions ==
curry :: ((a, b) -> c) -> a -> b -> c
  fst :: (a, b) -> a
  snd :: (a, b) -> b
   id :: a -> a
  (.) :: (a -> b) -> (c -> a) -> c -> b
  (|) :: (a -> b) -> (a -> c) -> a -> (b, c)

== Laws ==
  1. id x = x
  2. f . id = f
  3. id . f = f
  4. curry snd x = id
  5. fst | snd = id
  6. (f . g) x = f (g x)
  7. curry fst x y = x
  8. (id | id) x = curry id x x
  9. fst ((f | g) x) = f x
 10. snd ((f | g) x) = g x
 11. fst (curry id x y) = x
 12. snd (curry id x y) = y
 13. (f . g) . h = f . (g . h)
 14. curry (f . g) x = f . curry g x
 15. (id | id) (fst x) = (fst | fst) x
 16. (id | id) (snd x) = (snd | snd) x
 17. f . curry fst x = curry fst (f x)
 18. curry fst x . f = curry fst x
 19. fst . (f | g) = f
 20. snd . (f | g) = g
 21. (id | f) . g = g | (f . g)
 22. (f | id) . g = (f . g) | g
 23. id | curry f x = curry (snd | f) x
 24. curry f x | id = curry (f | snd) x
 25. curry fst x . snd = curry fst x . fst
 26. curry f (g x) x = f ((g | id) x)
 27. curry f (fst x) (snd x) = f x
 28. curry (snd | f) x x = curry (fst | f) x x
 29. curry (f | snd) x x = curry (f | fst) x x
 30. (f | curry fst x) y = curry id (f y) x
 31. curry f (g x) (fst x) = f ((g | fst) x)
 32. curry f (g x) (fst x) = f ((g | fst) x)
 33. curry f (g x) (snd x) = f ((g | snd) x)
 34. curry f (g x) (snd x) = f ((g | snd) x)
 35. curry f (fst x) (g x) = f ((fst | g) x)
 36. curry f (fst x) (g x) = f ((fst | g) x)
 37. curry f (snd x) (g x) = f ((snd | g) x)
 38. curry f (snd x) (g x) = f ((snd | g) x)
 39. curry id x (fst (f x)) = (id | (fst . f)) x
 40. curry id x (snd (f x)) = (id | (snd . f)) x
 41. curry id (fst (f x)) x = ((fst . f) | id) x
 42. curry id (snd (f x)) x = ((snd . f) | id) x
 43. (f . g) (curry id x y) = f (curry g x y)
 44. curry id x (curry f x y) = curry (fst | f) x y
 45. curry id x (curry f y x) = curry (snd | f) y x
 46. curry id (curry f x y) x = curry (f | fst) x y
 47. curry id (curry f x y) y = curry (f | snd) x y
 48. (snd | f) ((g | g) x) = (fst | f) ((g | g) x)
 49. (f | snd) ((g | g) x) = (f | fst) ((g | g) x)
 50. (f | g) (curry id x y) = curry (f | g) x y
 51. curry (f | f) x y = (id | id) (curry f x y)
 52. (snd | fst) ((f | g) x) = (g | f) x
 53. (f . g) | (h . g) = (f | h) . g
 54. (f | f) . curry fst x = curry (fst | fst) (f x)
 55. curry (f | (g . snd)) x = curry f x | g
 56. curry ((f . snd) | g) x = f | curry g x
 57. curry (fst | fst) x . snd = curry (fst | fst) x . fst

